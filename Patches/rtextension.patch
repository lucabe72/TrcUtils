+    req->rtimeReqType = X_RealTimeSetProperties;
+    req->id = client_resource_id;
+    req->capacity = capacity;
+    req->rttype = type;
+    req->period = period;
+    if (!_XReply(dpy, (xReply *) & rep, 0, xTrue)) {
+	UnlockDisplay(dpy);
+	SyncHandle();
+	return False;
+    }
+    UnlockDisplay(dpy);
+    SyncHandle();
+    return True;
