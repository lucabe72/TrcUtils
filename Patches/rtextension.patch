+static DISPATCH_PROC(SProcRealTimeSetProperties);
+
+
+
+ /*
+  * *****  Proc functions
+  */
+
+
+ /*
+  * ** Initialize the extension
+  */
+static int ProcRealTimeInitialize(client)
+ClientPtr client;
+{
+    xRealTimeInitializeReply rep;
+    int n;
+    REQUEST_SIZE_MATCH(xRealTimeInitializeReq);
+
+    rep.type = X_Reply;
+    rep.sequenceNumber = client->sequence;
+    rep.majorVersion = REALTIME_MAJOR_VERSION;
+    rep.minorVersion = REALTIME_MINOR_VERSION;
+    rep.length = 0;
+
+    if (client->swapped) {
+	swaps(&rep.sequenceNumber, n);
+    }
+    WriteToClient(client, sizeof(rep), (char *) &rep);
+    return (client->noClientException);
+}
+
+ /*
+  * ** Set client's rtime properties 
+  */
+static int ProcRealTimeSetProperties(client)
+ClientPtr client;
+{
+    xRealTimeSetPropertiesReply rep;
+    int n,rc;
+    ClientPtr rtclient;
+    REQUEST(xRealTimeSetPropertiesReq);
+    REQUEST_SIZE_MATCH(xRealTimeSetPropertiesReq);
+     
+    if (stuff->id == None)
+	rtclient = client;
+    else {
+	rc = dixLookupClient(&rtclient, stuff->id, client,
+			     DixUnknownAccess);
+	if (rc != Success)
+	    return rc;
+    } 
+
+    rtclient->rt_period=stuff->period;
+    rtclient->rt_capacity=stuff->capacity;
+    rtclient->rt_type=stuff->rttype;
+    rtclient->rt_deadline=GetTimeInMicros()+(rtclient->rt_period*1000);
+    rtclient->rt_capacity_residue=rtclient->rt_capacity*1000;
+    inc_rtclient();
+
+    rep.type = X_Reply;
+    rep.sequenceNumber = client->sequence;
+    rep.length = 0;
+    if (client->swapped) {
+	swaps(&rep.sequenceNumber, n);
+    }
+    WriteToClient(client, sizeof(rep), (char *) &rep);
+    return Success;
+}
+
+
+ /*
+  * ** Get client's properties
+  */
+
+static int ProcRealTimeGetProperties(client)
+ClientPtr client;
+{
+    REQUEST(xRealTimeGetPropertiesReq);
+    xRealTimeGetPropertiesReply rep;
+    int rc;
+    ClientPtr rtclient;
+    REQUEST_SIZE_MATCH(xRealTimeGetPropertiesReq);
+
+  if (stuff->id == None)
+	rtclient = client;
+    else {
+	rc = dixLookupClient(&rtclient, stuff->id, client,
+			     DixUnknownAccess);
+	if (rc != Success)
+	    return rc;
+    }
+    rep.type = X_Reply;
+    rep.length = 0;
+    rep.sequenceNumber = client->sequence;
+    rep.period = rtclient->rt_period;
+    rep.capacity = rtclient->rt_capacity;
+    rep.rttype = rtclient->rt_type;
+    if (client->swapped) {
+	register char n;
+	swaps(&rep.sequenceNumber, n);
+        swapl(&rep.period,n);
+        swapl(&rep.capacity,n);
+        swapl(&rep.rttype,n);
+    }
+    WriteToClient(client, sizeof(rep), (char *) &rep);
+    return client->noClientException;
+}
+
+ /*
+  * ** Given an extension request, call the appropriate request procedure
+  */
+static int ProcRealTimeDispatch(client)
+ClientPtr client;
+{
+    REQUEST(xReq);
+    switch (stuff->data) {
+    case X_RealTimeInitialize:
+	return ProcRealTimeInitialize(client);
+    case X_RealTimeSetProperties:
+	return ProcRealTimeSetProperties(client);
+    case X_RealTimeGetProperties:
+	return ProcRealTimeGetProperties(client);
+    default:
+	return BadRequest;
+    }
+}
+
+ /*
+  * Boring Swapping stuff ...
+  */
+
+static int SProcRealTimeInitialize(client)
+ClientPtr client;
+{
+    REQUEST(xRealTimeInitializeReq);
+    register char n;
+
+    swaps(&stuff->length, n);
+    REQUEST_SIZE_MATCH(xRealTimeInitializeReq);
+
+    return ProcRealTimeInitialize(client);
+}
+
+static int SProcRealTimeSetProperties(client)
+ClientPtr client;
+{
+    REQUEST(xRealTimeSetPropertiesReq);
+    register char n;
+
+    swaps(&stuff->length, n);
+    REQUEST_SIZE_MATCH(xRealTimeSetPropertiesReq);
+    swapl(&stuff->id, n);
+    return ProcRealTimeSetProperties(client);
+}
+
+static int SProcRealTimeGetProperties(client)
+ClientPtr client;
+{
+    REQUEST(xRealTimeGetPropertiesReq);
+    register char n;
+
+    swaps(&stuff->length, n);
+    REQUEST_SIZE_MATCH(xRealTimeGetPropertiesReq);
+    swapl(&stuff->id, n);
+    return ProcRealTimeGetProperties(client);
+}
+
+
+static int SProcRealTimeDispatch(client)
+ClientPtr client;
+{
+    REQUEST(xReq);
+    switch (stuff->data) {
+    case X_RealTimeInitialize:
+	return SProcRealTimeInitialize(client);
+    case X_RealTimeSetProperties:
+	return SProcRealTimeSetProperties(client);
+    case X_RealTimeGetProperties:
+	return SProcRealTimeGetProperties(client);
+    default:
+	return BadRequest;
+    }
+}
+
+ /*
+  * ** Close everything down. ** This is fairly simple for now.
+  */
+ /* ARGSUSED */
+static void RealTimeResetProc(extEntry)
+ExtensionEntry *extEntry;
+{
+}
+
+
+ /*
+  * ** Initialise the extension.
+  */
+void 
+RealTimeExtensionInit(INITARGS)
+{
+    ExtensionEntry *extEntry;
+    if ((extEntry = AddExtension(REALTIME_NAME,
+				 XRealTimeNumberEvents, XRealTimeNumberErrors,
+				 ProcRealTimeDispatch, SProcRealTimeDispatch,
+				 RealTimeResetProc,
+				 StandardMinorOpcode)) == NULL) {
+	ErrorF("Real Time Extension %d.%d failed to Initialise\n",
+	       REALTIME_MAJOR_VERSION, REALTIME_MINOR_VERSION);
+	return;
+    }
+
+    RealTimeEventBase = extEntry->eventBase;
+    RealTimeErrorBase = extEntry->errorBase;
+    //    EventSwapVector[RTimeEventBase   XRTimeNotifyEvent] = (EventSwapPtr) SRTimeNotifyEvent;
+
+#ifdef DEBUG
+    fprintf(stderr, "Real Time Extension %d.%d\n",
+	    REALTIME_MAJOR_VERSION, REALTIME_MINOR_VERSION);
+#endif
+}
+
+
+void inc_rtclient(){
+   rtcount+=1;
+};
+void dec_rtclient(){
+   if (rtcount>0)
+   	rtcount-=1;
+	else fprintf(stderr,"Error realtime extension");
+};
+
+int is_enabled_rtclient(){
+   if (rtcount>0)
+   	return 1;
+	return 0;
+};
+
+void add_requestingclient(int client){
+	pClientsRealTimeReady[nrealtimeready]=client;
+	nrealtimeready++;
+};
+
+int *get_requestingclients(){
+	int *temp;
+	temp=(int *)malloc(sizeof(int)*nrealtimeready);
+	memcpy(temp,pClientsRealTimeReady,nrealtimeready*sizeof(int));
+	return temp;
+};
+
+int get_numrequestingclients(){
+	return nrealtimeready;
+}
+
+void reset_rtclients(){
+	nrealtimeready=0;
+}
+
+void alloc_rtclients(){
+	pClientsRealTimeReady = (int *) ALLOCATE_LOCAL(sizeof(int) * MaxClients);
+}
+
+void dealloc_rtclients(){
+	DEALLOCATE_LOCAL(pClientsRealTimeReady);
+}
