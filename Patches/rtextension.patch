+    GetReq(RealTimeInitialize, req);
+    req->reqType = info->codes->major_opcode;
+    req->rtimeReqType = X_RealTimeInitialize;
+    req->majorVersion = REALTIME_MAJOR_VERSION;
+    req->minorVersion = REALTIME_MINOR_VERSION;
