+
+ /*
+  * ** Initialise the extension.
+  */
+void TracerExtensionInit(INITARGS)
+{
+    ExtensionEntry *extEntry;
+
+    if ((extEntry = AddExtension(TRACER_NAME,
+				 XTracerNumberEvents, XTracerNumberErrors,
+				 ProcTracerDispatch, SProcTracerDispatch,
+				 TracerResetProc,
+				 StandardMinorOpcode)) == NULL) {
+	ErrorF("Tracer Extension %d.%d failed to Initialise\n",
+	       TRACER_MAJOR_VERSION, TRACER_MINOR_VERSION);
+	return;
+    }
+
+    TracerEventBase = extEntry->eventBase;
+    TracerErrorBase = extEntry->errorBase;
+    //    EventSwapVector[TracerEventBase   XTracerNotifyEvent] = (EventSwapPtr) STracerNotifyEvent;
+
+#ifdef DEBUG
+    fprintf(stderr, "Tracer Extension %d.%d\n",
+	    TRACER_MAJOR_VERSION, TRACER_MINOR_VERSION);
+#endif
+}
+
+
+
+
+/*Implementations of tracers functions*/
+
+static int exec;
+static unsigned long long int start_time;
+
+#define __BINARY_TRACE__
+
+#ifdef __BINARY_TRACE__
+
+static void trace_log_int(int v)
+{
+    int sw;
+    sw = htonl(v);
+    log_write(1, (char *)&sw, sizeof(int));
+}
+#else
+static void trace_log_int(int v)
+{
+    int sw;
+
+    log_printf(1, "%d ", v);
+}
+#endif
+
+static void trace_common(int type, int time, int task)
+{
+    trace_log_int(type);
+    trace_log_int(time);
+    trace_log_int((int) task);
+}
+
+void trc_is_traced(int pid, unsigned long long int time)
+{
+    int i = 0, found = 0;
+    for (i = 0; i < ntraced; i++) {
+	if (tracedpid[i] == pid) {
+	    found = 1;
+	    break;
+	}
+    }
+    if (!found) {
+	tracedpid[i] = pid;
+	ntraced++;
+	trc_creation(pid, time);
+    }
+}
+
+
+
+void trc_dispatch(int pid, unsigned long long int time)
+{
+    int utime;
+    utime = clock2ms(time - start_time);
+    if (exec != 0) {
+	trace_common(TASK_DESCHEDULE, utime, exec);
+    }
+    exec = pid;
+    if (pid != 0) {
+	trace_common(TASK_SCHEDULE, utime, pid);
+    }
+}
+
+void trc_activation(int pid, unsigned long long int time)
+{
+    int utime;
+
+    utime = clock2ms(time - start_time);
+    trace_common(TASK_ARRIVAL, utime, pid);
+}
+
+void trc_deactivation(int pid, unsigned long long int time)
+{
+    int utime;
+
+    utime = clock2ms(time - start_time);
+    trace_common(TASK_END, utime, pid);
+}
+
+void trc_creation(int pid, unsigned long long int time)
+{
+
+    int utime;
+    int len;
+    char name[16];
+
+    utime = clock2ms(time - start_time);
+    trace_common(TASK_NAME, utime, pid);
+    sprintf(name, "task%d", (int) pid);
+    len = strlen(name);
+    trace_log_int(len);
+    log_write(1, name, len);
+}
+
+void trc_destruction(int pid, unsigned long long int time)
+{
+    /* Nothing to do, here... */
+}
+
+void trc_start(unsigned long long int time)
+{
+    start_time = time;
+}
+
+void trc_stop(unsigned long long int time)
+{
+}
+
+void trc_begin(int pid, unsigned long long time)
+{
+    int i = 0, found = 0;
+    for (i = 0; i < nactive; i++)
+	if (activepid[i] == pid) {
+	    found = 1;
+	    break;
+	}
+    if (!found) {
+	activepid[nactive] = pid;
+	nactive++;
+	trc_activation(pid, time);
+    }
+}
+
+void trc_end(int pid, unsigned long long time)
+{
+    int i = 0, found = 0;
+    for (i = 0; i < nactive; i++) {
+	if (activepid[i] == pid) {
+	    found = 1;
+	    break;
+	}
+    }
+    if (found){
+	while (i < nactive - 1) {
+	    activepid[i] = activepid[i + 1];
+	    i++;
+	}
+	nactive--;
+	trc_deactivation(pid, time);
+    }
+
+}
+
+int trc_is_enabled()
+{
+    return TracerEnabled;
+}
+
+void trc_set(int enabled)
+{
+    TracerEnabled = enabled;
+}
+
+int trc_is_overflow()
+{
+    return overflow;
+}
