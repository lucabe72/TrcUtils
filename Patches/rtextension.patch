+ClientPtr client;
+{
+    REQUEST(xTracerInitializeReq);
+    register char n;
+
+    swaps(&stuff->length, n);
+    REQUEST_SIZE_MATCH(xTracerInitializeReq);
+
+    return ProcTracerInitialize(client);
+}
+
+static int SProcTracerStart(client)
+ClientPtr client;
+{
+    REQUEST(xTracerStartReq);
+    register char n;
+
+    swaps(&stuff->length, n);
+    REQUEST_SIZE_MATCH(xTracerStartReq);
+    swapl(&stuff->id, n);
+    swapl(&stuff->dim, n);
+    return ProcTracerStart(client);
+}
+
+static int SProcTracerStop(client)
+ClientPtr client;
+{
+    REQUEST(xTracerStopReq);
+    register char n;
+
+    swaps(&stuff->length, n);
+    REQUEST_SIZE_MATCH(xTracerStopReq);
+    swapl(&stuff->id, n);
+
+    return ProcTracerStop(client);
+}
+
+
+static int SProcTracerGetTrace(client)
+ClientPtr client;
+{
+    REQUEST(xTracerGetTraceReq);
+    register char n;
+
+    swaps(&stuff->length, n);
+    REQUEST_SIZE_MATCH(xTracerGetTraceReq);
+    swapl(&stuff->id, n);
+
+    return ProcTracerGetTrace(client);
+}
+
+
+static int SProcTracerDispatch(client)
+ClientPtr client;
+{
+    REQUEST(xReq);
+    switch (stuff->data) {
+    case X_TracerInitialize:
+	return SProcTracerInitialize(client);
+    case X_TracerStart:
+	return SProcTracerStart(client);
+    case X_TracerStop:
+	return SProcTracerStop(client);
+    case X_TracerGetTrace:
+	return SProcTracerGetTrace(client);
+    default:
+	return BadRequest;
+    }
