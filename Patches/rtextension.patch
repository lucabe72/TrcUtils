+static DISPATCH_PROC(SProcTracerGetTrace);
+static DISPATCH_PROC(SProcTracerInitialize);
+static DISPATCH_PROC(SProcTracerStart);
+static DISPATCH_PROC(SProcTracerStop);
+
+
+
+ /*
+  * *****  Proc functions
+  */
+
+
+ /*
+  * ** Initialize the extension
+  */
+static int ProcTracerInitialize(client)
+ClientPtr client;
+{
+    xTracerInitializeReply rep;
+    int n;
+    REQUEST_SIZE_MATCH(xTracerInitializeReq);
+
+    rep.type = X_Reply;
+    rep.sequenceNumber = client->sequence;
+    rep.majorVersion = TRACER_MAJOR_VERSION;
+    rep.minorVersion = TRACER_MINOR_VERSION;
+    rep.length = 0;
+
+    if (client->swapped) {
+	swaps(&rep.sequenceNumber, n);
+    }
+    WriteToClient(client, sizeof(rep), (char *) &rep);
+    return (client->noClientException);
+}
+
+ /*
+  * ** Start Tracer
+  */
+static int ProcTracerStart(client)
+ClientPtr client;
+{
+    xTracerStartReply rep;
+    int n;
+    REQUEST(xTracerStartReq);
+    REQUEST_SIZE_MATCH(xTracerStartReq);
+
+    dim_buffer = stuff->dim;
+    trc_set(1);
+    /*Start tracer and set the dimension that is stuff->dim */
+    log_buffer = (char *) malloc(dim_buffer);
+    trc_start(GetTimeInMicros());
+    p=0;
+    overflow=0;
+    nactive=0;
+    ntraced=0;
+    rep.type = X_Reply;
+    rep.sequenceNumber = client->sequence;
+    rep.length = 0;
+    if (client->swapped) {
+	swaps(&rep.sequenceNumber, n);
+    }
+    WriteToClient(client, sizeof(rep), (char *) &rep);
+    return Success;
+}
+
+ /*
+  * ** Stop Tracer
+  */
+static int ProcTracerStop(client)
+ClientPtr client;
+{
+    xTracerStopReply rep;
+    int n;
+    REQUEST_SIZE_MATCH(xTracerStopReq);
+    /*Stop the tracer */
+    trc_set(0); 
+    trc_stop(GetTimeInMicros());
+    free(log_buffer);
+    rep.type = X_Reply;
+    rep.sequenceNumber = client->sequence;
+    rep.length = 0;
+    if (client->swapped) {
+	swaps(&rep.sequenceNumber, n);
+    }
+    WriteToClient(client, sizeof(rep), (char *) &rep);
+    return Success;
+}
+
+
+
+ /*
+  * ** Get the trace
+  */
+static int ProcTracerGetTrace(client)
+ClientPtr client;
+{
+    xTracerGetTraceReply rep;
+    int err=Success;
+
+    REQUEST_SIZE_MATCH(xTracerGetTraceReq);
+
+    rep.type = X_Reply;
+    rep.length = 0;
+    rep.sequenceNumber = client->sequence;
+    rep.dim=dim_buffer;
+    rep.overflow=trc_is_overflow();
+    WindowPtr root=GetCurrentRootWindow();
+    err = ChangeWindowProperty(root, XA_CUT_BUFFER0, XA_STRING, 8,
+			       PropModeReplace, p, (pointer)log_buffer, FALSE);
+
+    if (client->swapped) {
+	register char n;
+	swaps(&rep.sequenceNumber, n);
+	swapl(&rep.trace, n);
+    }
+    WriteToClient(client, sizeof(xTracerGetTraceReply), (char *) &rep);
+    if (err != Success)
+	return err;
+    else
+	return client->noClientException;
+
+}
